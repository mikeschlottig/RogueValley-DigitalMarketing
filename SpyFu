    <script>
        // Navigation functionality
        function showSection(sectionId) {
            // Hide all sections
            document.querySelectorAll('.content-section').forEach(section => {
                section.classList.remove('active');
            });
            
            // Remove active class from all tabs
            document.querySelectorAll('.nav-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Show selected section
            document.getElementById(sectionId).classList.add('active');
            
            // Add active class to corresponding tab
            document.querySelector(`[data-section="${sectionId}"]`).classList.add('active');
        }

        // Tab navigation
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', function() {
                const sectionId = this.dataset.section;
                showSection(sectionId);
            });
        });

        // Step completion
        function completeStep(stepElement) {
            stepElement.classList.add('completed');
            
            // Update progress
            const steps = stepElement.parentElement.querySelectorAll('.step');
            const completedSteps = stepElement.parentElement.querySelectorAll('.step.completed');
            const progressPercent = (completedSteps.length / steps.length) * 100;
            
            // Update progress bar if it exists
            const progressBar = document.querySelector('.progress-fill');
            if (progressBar) {
                progressBar.style.width = Math.max(progressPercent, 10) + '%';
            }
        }

        // Checkbox functionality
        function toggleCheckbox(item) {
            const checkbox = item.querySelector('.checkbox');
            checkbox.classList.toggle('checked');
        }

        // Workflow step toggle
        function toggleWorkflowStep(step) {
            step.classList.toggle('completed');
        }

        // Navigation simulator
        function highlightNavItem(item, section) {
            // Remove active class from all items
            document.querySelectorAll('.simulator-nav-item').forEach(navItem => {
                navItem.classList.remove('active');
            });
            
            // Add active class to clicked item
            item.classList.add('active');
            
            // Update content based on section
            const contentArea = document.getElementById('nav-content');
            const sectionContent = {
                'dashboard': {
                    title: 'Dashboard',
                    description: 'Your SpyFu command center with quick access to all competitive intelligence tools'
                },
                'keyword-research': {
                    title: 'Keyword Research',
                    description: 'Discover and analyze keywords with difficulty scoring and competitive intelligence'
                },
                'seo-research': {
                    title: 'SEO Research',
                    description: 'Comprehensive SEO analysis including Kombat tool and ranking history'
                },
                'ppc-research': {
                    title: 'PPC Research',
                    description: 'Competitor PPC intelligence including ad history and keyword bidding data'
                },
                'projects': {
                    title: 'Projects',
                    description: 'Organize and track your competitive intelligence campaigns'
                },
                'reports': {
                    title: 'Reports',
                    description: 'Generate white-label reports and track performance metrics'
                }
            };
            
            const content = sectionContent[section];
            contentArea.innerHTML = `
                <div class="metric-box">
                    <div class="metric-value">${content.title}</div>
                    <div class="metric-label">${content.description}</div>
                </div>
            `;
        }

        // Kombat Analysis
        function runKombatAnalysis() {
            const yourDomain = document.getElementById('your-domain-kombat').value;
            const competitor1 = document.getElementById('competitor1-kombat').value;
            const competitor2 = document.getElementById('competitor2-kombat').value;
            
            if (!yourDomain || !competitor1 || !competitor2) {
                alert('Please enter all three domains for Kombat analysis');
                return;
            }
            
            document.getElementById('kombat-results').style.display = 'block';
        }

        function showKeywordSegment(segment) {
            const messages = {
                'your-unique': 'Your Unique Keywords: These are keywords only you rank for. Consider if competitors should be targeting these too.',
                'shared': 'Shared Keywords: These are competitive battlegrounds where all players fight for rankings.',
                'competitor-unique': 'Competitor Keywords: These are your biggest opportunities - keywords competitors rank for but you don\'t!'
            };
            
            alert(messages[segment]);
        }

        function explainKombatSegments() {
            document.getElementById('kombat-explanation').style.display = 'block';
        }

        function showKombatTips() {
            alert('SpyFu Kombat Pro Tips:\n\n‚úì Focus on "Consider Buying" segment for validated opportunities\n‚úì Multiple competitors targeting same keywords = market validation\n‚úì Use historical data to see when competitors started targeting keywords\n‚úì Look for seasonal patterns in competitor targeting');
        }

        function startKombatChallenge() {
            alert('üèÜ Kombat Challenge Started!\n\nTask: Use the Kombat tool to find 10 high-value keywords that 2+ competitors target but you don\'t.\n\nSuccess Criteria:\n- Keywords with search volume >1,000\n- Difficulty score <50\n- Commercial intent indicators\n\nTime limit: 30 minutes');
        }

        // Keyword Research
        function analyzeKeywordDifficulty() {
            const keyword = document.getElementById('keyword-difficulty-input').value;
            if (!keyword) {
                alert('Please enter a keyword to analyze');
                return;
            }
            
            document.getElementById('keyword-difficulty-results').style.display = 'block';
            
            // Simulate realistic data
            setTimeout(() => {
                const difficulty = Math.floor(Math.random() * 100) + 1;
                const volume = Math.floor(Math.random() * 20000) + 1000;
                const competition = difficulty < 30 ? 'Low' : difficulty < 60 ? 'Medium' : 'High';
                const opportunity = difficulty < 40 && volume > 2000 ? 'High' : difficulty < 60 ? 'Medium' : 'Low';
                
                document.getElementById('difficulty-score').textContent = difficulty;
                document.getElementById('search-volume-kw').textContent = volume.toLocaleString();
                document.getElementById('competition-level').textContent = competition;
                document.getElementById('opportunity-score').textContent = opportunity;
            }, 500);
        }

